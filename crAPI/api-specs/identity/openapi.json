    "components": {
        "schemas": {
            "VideoForm": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int64"
                    },
                    "videoName": {
                        "type": "string"
                    },
                    "video_url": {
                        "type": "string"
                    },
                    "conversion_params": {
                        "type": "string"
                    }
                }
            },
            "CRAPIResponse": {
                "type": "object",
                "properties": {
                    "message": {
                        "type": "string"
                    },
                    "status": {
                        "type": "integer",
                        "format": "int32"
                    }
                }
            },
            "VehicleForm": {
                "required": [
                    "pincode",
                    "vin"
                ],
                "type": "object",
                "properties": {
                    "pincode": {
                        "maxLength": 8,
                        "minLength": 4,
                        "type": "string"
                    },
                    "vin": {
                        "maxLength": 20,
                        "minLength": 4,
                        "type": "string"
                    }
                }
            },
            "ChangeEmailForm": {
                "required": [
                    "new_email",
                    "old_email"
                ],
                "type": "object",
                "properties": {
                    "old_email": {
                        "maxLength": 40,
                        "minLength": 3,
                        "type": "string"
                    },
                    "new_email": {
                        "maxLength": 40,
                        "minLength": 3,
                        "type": "string"
                    },
                    "token": {
                        "type": "string"
                    }
                }
            },
            "LoginForm": {
                "required": [
                    "email",
                    "password"
                ],
                "type": "object",
                "properties": {
                    "email": {
                        "maxLength": 60,
                        "minLength": 3,
                        "type": "string"
                    },
                    "password": {
                        "maxLength": 40,
                        "minLength": 4,
                        "type": "string"
                    },
                    "number": {
                        "type": "string"
                    }
                }
            },
            "LoginWithEmailToken": {
                "required": [
                    "email",
                    "token"
                ],
                "type": "object",
                "properties": {
                    "email": {
                        "maxLength": 60,
                        "minLength": 3,
                        "type": "string"
                    },
                    "token": {
                        "maxLength": 60,
                        "minLength": 3,
                        "type": "string"
                    }
                }
            },
            "OtpForm": {
                "required": [
                    "email",
                    "otp",
                    "password"
                ],
                "type": "object",
                "properties": {
                    "otp": {
                        "maxLength": 4,
                        "minLength": 3,
                        "type": "string"
                    },
                    "password": {
                        "maxLength": 30,
                        "minLength": 5,
                        "type": "string"
                    },
                    "email": {
                        "maxLength": 30,
                        "minLength": 5,
                        "type": "string"
                    }
                }
            },
            "JwtResponse": {
                "type": "object",
                "properties": {
                    "token": {
                        "type": "string"
                    },
                    "type": {
                        "type": "string"
                    },
                    "message": {
                        "type": "string"
                    },
                    "role": {
                        "type": "string",
                        "enum": [
                            "ROLE_UNDEFINED",
                            "ROLE_USER",
                            "ROLE_MECHANIC",
                            "ROLE_ADMIN"
                        ]
                    }
                }
            },
            "SignUpForm": {
                "required": [
                    "email",
                    "name",
                    "number",
                    "password"
                ],
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int64"
                    },
                    "name": {
                        "maxLength": 40,
                        "minLength": 3,
                        "type": "string"
                    },
                    "password": {
                        "maxLength": 40,
                        "minLength": 6,
                        "type": "string"
                    },
                    "email": {
                        "maxLength": 60,
                        "minLength": 0,
                        "type": "string"
                    },
                    "number": {
                        "maxLength": 15,
                        "minLength": 0,
                        "type": "string"
                    }
                }
            },
            "ForgetPassword": {
                "required": [
                    "email"
                ],
                "type": "object",
                "properties": {
                    "email": {
                        "maxLength": 60,
                        "minLength": 3,
                        "type": "string"
                    }
                }
            }
        },
        "securitySchemes": {
            "JWT": {
                "type": "http",
                "name": "Authorization",
                "in": "header",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    }
